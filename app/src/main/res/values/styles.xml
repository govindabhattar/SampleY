        <attr name="belowTextColor" format="color" />
        <attr name="compoundDrawablePadding" format="dimension" />
        <attr name="viewMargin" format="dimension" />
        <attr name="viewMinWidth" format="dimension" />
        <attr name="onlyForDisplay" format="boolean" />
        <attr name="halfStars" format="boolean" />
        <attr name="animate" format="boolean" />
        <attr name="displayText" format="boolean" />
        <attr name="singleColor" format="boolean" />
        <attr name="starColor" format="color" />
        <attr name="noStarColor" format="color" />
    </declare-styleable>


    <style name="tvPDP">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textSize">@dimen/text_size_medium</item>
        <item name="android:textAppearance">@style/CustomTextAppearance.Light</item>
        <item name="android:textColor">@color/primary_text_color</item>
    </style>

    <style name="blank_view_pdp">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">1dp</item>
        <item name="android:layout_marginTop">@dimen/_14sp</item>
        <item name="android:background">@color/seperator_color</item>
    </style>

    <!--Calender style-->
    <style name="WeekDayTitleStyle">
        <item name="android:layout_width">@dimen/dayOfMonthBoxSize</item>
        <item name="android:layout_height">@dimen/dayOfMonthBoxSize</item>
        <item name="android:textSize">@dimen/text_size_week</item>
        <item name="android:gravity">center</item>
        <item name="android:textColor">@color/colorAccent</item>
        <item name="android:paddingBottom">@dimen/dayOfMonthPaddingBottom</item>
    </style>

    <style name="DayOfMonthStyle">
        <item name="android:layout_width">@dimen/dayOfMonthSize</item>
        <item name="android:layout_height">@dimen/dayOfMonthSize</item>
        <item name="android:textSize">@dimen/text_size_date</item>
        <item name="android:gravity">center</item>
        <item name="android:minHeight">@dimen/dayOfMonthMinHeight</item>
        <item name="android:minWidth">@dimen/dayOfMonthMinWidth</item>
        <item name="android:layout_centerInParent">true</item>
        <item name="android:paddingBottom">@dimen/dayOfMonthPaddingBottom</item>
        <item name="android:textColor">@color/default_date_color</item>
    </style>

    <style name="ContainerDayOfWeek">
        <item name="android:layout_width">@dimen/dayOfMonthBoxSize</item>
        <item name="android:layout_height">@dimen/dayOfMonthBoxSize</item>
        <item name="android:gravity">center_vertical</item>
    </style>

    <style name="StripRange">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">@dimen/strip_height</item>
        <item name="android:background">@color/range_bg_color</item>
        <item name="android:layout_centerVertical">true</item>


    </style>

    <!--Calendar style-->
    <style name="CalendarTitle">
        <item name="android:textSize">@dimen/calendar_text_large</item>
        <item name="android:textStyle">bold</item>
    </style>

    <style name="CalendarCell">
        <item name="android:layout_width">0dp</item>
        <item name="android:layout_weight">1</item>
        <item name="android:gravity">center</item>
        <item name="android:textAlignment" tools:targetApi="jelly_bean_mr1">center</item>
    </style>

    <style name="CalendarCell.DayHeader">
        <item name="android:textStyle">bold</item>
        <item name="android:textSize">@dimen/calendar_text_small</item>
    </style>

    <style name="CalendarCell.CalendarDate">
        <item name="android:textSize">@dimen/calendar_text_medium</item>
    </style>

    <!--Subscribe style-->
    <style name="SubscribeRadioButton">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:padding">@dimen/_15dp</item>
        <item name="android:textColor">@color/black</item>
        <item name="android:button">@drawable/custom_btn_radio</item>
        <item name="android:textSize">@dimen/text_size_medium</item>
    </style>

    <style name="AppTheme.NoActionBar">
        <item name="windowActionBar">false</item>
        <item name="windowNoTitle">true</item>
    </style>

    <style name="AppTheme.AppBarOverlay" parent="ThemeOverlay.AppCompat.Dark.ActionBar" />

    <style name="AppTheme.PopupOverlay" parent="ThemeOverlay.AppCompat.Light" />

    <style name="DialogStyle" parent="Theme.Design.Light.BottomSheetDialog">
        <item name="android:windowIsFloating">false</item>
        <item name="android:statusBarColor" tools:ignore="NewApi">@android:color/transparent</item>
        <item name="android:windowSoftInputMode">adjustResize</item>
    </style>
    <style name="SpinnerTheme" parent="android:Widget.Spinner">
        <item name="android:background">@drawable/spinner_background</item>
    </style>
</resources>
